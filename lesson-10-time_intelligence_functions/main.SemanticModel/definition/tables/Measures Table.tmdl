	measure 'MoM %' =
			
			VAR current_month_sales = [Total amount]
			VAR prev_month_sales = CALCULATE([Total amount],DATEADD('Date'[Date],-1,MONTH))
			RETURN IF(NOT(ISBLANK(current_month_sales)) && NOT(ISBLANK(prev_month_sales)), DIVIDE(current_month_sales-prev_month_sales, prev_month_sales, BLANK()))
		formatString: 0.00%;-0.00%;0.00%
		displayFolder: MoM/QoQ/YoY
		lineageTag: 9689b1c2-a3dc-42d8-abe0-287b8c7b4ee3

	measure prev_month_sales = CALCULATE([Total amount],DATEADD('Date'[Date],-1,MONTH))
		displayFolder: Total
		lineageTag: 29d06671-32be-4991-8bb2-9da72f07b8cf

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'QoQ %' =
			
			VAR current_quarter_sales = [Total amount]
			VAR prev_quarter_sales = CALCULATE([Total amount],DATEADD('Date'[Date],-1,QUARTER))
			RETURN IF(NOT(ISBLANK(current_quarter_sales)) && NOT(ISBLANK(prev_quarter_sales)), DIVIDE(current_quarter_sales-prev_quarter_sales, prev_quarter_sales, BLANK()))
