	measure 'Total amount (Prev month)' = CALCULATE([Total amount], PREVIOUSMONTH('Date'[Date]))
		lineageTag: 1a002d71-895c-4208-90aa-8910ba56da50

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'MTDoPM %' =
			
			var current_mtd = [Total amount (DateSMTD)]
			var prev_month = [Total amount (Prev month)]
			RETURN IF(NOT(ISBLANK(current_mtd)) && NOT(ISBLANK(prev_month)), DIVIDE(current_mtd-prev_month,prev_month), BLANK())
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: a0a2661f-ed0d-408d-aa9b-add5bc800eab

